{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Run docker-compose up",
        "type": "docker-compose",
        "dockerCompose": {
          "up": {
            "detached": true,
            "build": true
          },
          "files": [
            "${workspaceFolder}/compose.yaml"
          ]
        }
      },
      {
        "label": "Run docker-compose down",
        "type": "docker-compose",
        "dockerCompose": {
          "down": {
          },
          "files": [
            "${workspaceFolder}/compose.yaml"
          ]
        }
      }
    ]
  }
  